<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    
	<CheckBoxPreference
        android:key="pref_facebook_login"
        android:title="@string/pref_facebook_login"
        android:summary="@string/pref_facebook_login_summ"
        android:defaultValue="true" />
    <ListPreference
        android:key="pref_lang"
        android:title="@string/pref_language"
        android:dialogTitle="@string/pref_language_title"
        android:entries="@array/pref_language_entries"
        android:entryValues="@array/pref_language_values"
        android:defaultValue="@string/pref_language_default" />
</PreferenceScreen>
